#include "SerialTransfer.h"

SerialTransfer myTransfer;
int status;

void setup()
{
  Serial.begin(115200);
  myTransfer.begin(Serial);
  pinMode(5, INPUT);
}

void loop()
{
  status = digitalRead(5);
	status = 1; // for testing
  myTransfer.txObj(status, sizeof(status));
  myTransfer.sendData(sizeof(status));
  //delay(100);
	delay(500);
}
